{
  "name": "regl-cloudtest",
  "version": "1.0.0",
  "description": "Clouds using raymarching",
  "main": "src/index.js",
  "scripts": {
    "test": "test",
    "clean": "rm bundle.js",
    "build" : "watchify -t [ glslify --transform glslify-import ] ./src/index.ts -p [ tsify ] -o bundle.js",
    "start": "browserify -t [ glslify --transform glslify-import ] ./src/index.ts -p [ tsify ] -o bundle.js && http-server . -c-1 -a localhost -p 8000."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maydit/regl-cloudtest"
  },
  "keywords": [
    "webgl",
    "regl"
  ],
  "author": "David May",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Maydit/regl-cloudtest/issues"
  },
  "homepage": "https://github.com/Maydit/regl-cloudtest#readme",
  "dependencies": {
    "budo": "^11.2.2",
    "bunny": "^1.0.1",
    "gl-mat4": "^1.2.0",
    "glslify": "^6.1.1",
    "http-server": "^0.11.1",
    "regl": "^1.3.0",
    "resl": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "browserify": "^15.0.0",
    "tsify": "^3.0.4",
    "typescript": "^2.6.2"
  }
}
